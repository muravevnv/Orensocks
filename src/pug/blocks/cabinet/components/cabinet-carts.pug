.cabinet__section.js-tabs-content(data-content="c-carts") 
  .cabinet__section-title Сохранённые корзины
  .cabinet__section-content 
    .cabinet-carts
      .cabinet-carts__notification 
        .cabinet-carts__notification-title Корзина 30.03.2024 13:13 загружена
        a(href="#").cabinet-carts__notification-order 
          span Перейти к заказу
          +svg(24, 24, 'arrow-right')
        button(type="button").cabinet-carts__notification-close
          +svg(24, 24, 'close')
      .cabinet-carts__filters.cabinet-filters
        .cabinet-filters__choose-all 
          .form-checkbox 
            input(type="checkbox", name="" id="c_carts_all")
            label(for="c_carts_all") Выбрать все
        button(type="button" disabled).btn.btn--outline.cabinet-filters__remove-choosen 
          +svg(24, 24, 'trash')
          span Удалить выбранные
      .cabinet-carts__table.cabinet__table.table
        table 
          thead
            tr 
              th 
              th 
                .table-cell-head 
                  .table-cell-head__name Дата
                  .table-cell-head__sorting 
                    button(type="button").table-cell-head__sorting-up
                      +svg(8, 5, 'polygon-up')
                    button(type="button").table-cell-head__sorting-down
                      +svg(8, 5, 'polygon-down')
              th 
                .table-cell-head 
                  .table-cell-head__name Название
                  .table-cell-head__sorting 
                    button(type="button").table-cell-head__sorting-up
